<h1> Reset your password </h1>

<%= form_with url: password_reset_edit_path do |form|%>

<div class ="mb-3">
<%= form.label :Password%>
<%= form.password_field :password , class:"form-control", placeholder: "1!Password" %>
</div>
  
<div class ="mb-3">
<%= form.submit "Reset Password",  class: "btn btn-primary"%>
</div>
  
<%end%>